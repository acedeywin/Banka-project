{
  "name": "banka-project",
  "version": "1.0.0",
  "description": "core banking app",
  "main": "server.js",
  "scripts": {
    "start": "node server/dist/app.js",
    "build": "babel server -d server/dist",
    "start-dev": "nodemon --exec babel-node server/server.js",
    "test": "nyc --require babel-core/register mocha server/test/**.test.js --exit -w",
    "test-single": "nyc --require babel-core/register mocha server/test/**.test.js --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "bin": {
    "coveralls": "./bin/coveralls.js"
  },
  "author": "acedeywin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/acedeywin/Banka-project/issues"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.3",
    "mocha": "^6.1.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
